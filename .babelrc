{
  "env": {
    "commonjs": {
      "presets": [
        ["env", {
          "useBuiltIns": false
        }]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "external-helpers",
        "transform-class-properties"
      ]
    },
    "es": {
      "presets": [
        ["env", {
          "useBuiltIns": false,
          "modules": false
        }]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "external-helpers",
        "transform-class-properties"
      ]
    }
  }
}